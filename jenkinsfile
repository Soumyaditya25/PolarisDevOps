pipeline{
    agent any
    stages 
        stage('Checkout') {
            steps {
                git url: 'https://github.com/Soumyaditya25/PolarisDevOps', branch: 'master'
            }
        }
    stage('Build') {
        steps {
            script {
                echo 'Building...'
            }
        }
    }
    stage('Test') {
        steps {
            script {
                echo 'Testing...'
            }
        }
    }
    stage('Deploy') {
        steps {
            script {
                echo 'Deploying...'
            }
        }
    }
}
